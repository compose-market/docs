            "pages": [
              "endpoints/agents/chat",
              "endpoints/agents/register"
            ]
          },
          {
            "group": "On-Chain (Manowar)",
            "pages": [
              "endpoints/manowar/get-agent",
              "endpoints/manowar/invoke-agent"
            ]
          },
          {
            "group": "Registry (Connector)",
            "pages": [
              "endpoints/connector/list"
            ]
          },
          {
            "group": "Utilities",
            "pages": [
              "endpoints/utilities/run-workflow"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://docs.compose.market",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://blog.compose.market",
          "icon": "newspaper"
        }
      ]
